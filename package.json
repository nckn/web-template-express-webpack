{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "A simple boileplate for express websites",
  "main": "app.js",
  "scripts": {
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\""
  },
  "author": "Niels Konrad",
  "license": "MIT",
  "dependencies": {
    "@prismicio/client": "^5.1.0",
    "bootstrap": "^5.1.0",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "popper.js": "^1.16.1",
    "prismic-dom": "^2.2.6",
    "pug": "^3.0.2",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "auto-bind": "^4.0.0",
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.7.1",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "postcss-loader": "^2.0.10",
    "raw-loader": "^4.0.2",
    "sass-loader": "^6.0.6",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.49.0"
  }
}
